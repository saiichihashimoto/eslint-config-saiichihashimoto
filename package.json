{
  "name": "eslint-config-saiichihashimoto",
  "version": "0.0.0-development",
  "keywords": [
    "check",
    "code",
    "eslint",
    "eslint-config",
    "eslintconfig",
    "fix",
    "format",
    "javascript",
    "lint",
    "optimize",
    "quality",
    "validate"
  ],
  "homepage": "https://github.com/saiichihashimoto/eslint-config-saiichihashimoto#readme",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/eslint-config-saiichihashimoto/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saiichihashimoto/eslint-config-saiichihashimoto.git"
  },
  "license": "MIT",
  "main": ".eslintrc.js",
  "scripts": {
    "test": "run-s test:*",
    "pretest:fail": "rm -f fail/*.js && for i in fail/*; do cp $i $i.js; done",
    "test:fail": "for i in fail/*.js; do echo \"eslint $i\" should fail; ! eslint $i; done",
    "posttest:fail": "rm fail/*.js",
    "test:find-rules": "eslint-find-rules --unused --deprecated",
    "test:success": "eslint success"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "babel-eslint": "10.0.2",
    "confusing-browser-globals": "1.0.7",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.13.7",
    "eslint-plugin-jest-formatting": "0.1.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "eslint-plugin-unicorn": "10.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.5.0",
    "eslint-plugin-you-dont-need-momentjs": "1.5.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.0.2",
    "eslint": "5.16.0",
    "eslint-find-rules": "3.3.1",
    "husky": "3.0.1",
    "lint-my-app": "3.5.3",
    "npm-run-all": "4.1.5",
    "react": "16.8.6"
  },
  "peerDependencies": {
    "eslint": ">=5.0.0 <6.0.0"
  },
  "engines": {
    "node": "12.6.0"
  }
}
